{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "docs/diagrams",
  "name": "CIA Compliance Manager Diagrams",
  "includeVersion": true,
  "plugin": ["typedoc-plugin-mermaid", "typedoc-plugin-markdown"],
  "excludeExternals": false,
  "mermaidVersion": "10.6.1",
  "readme": "./docs/diagrams-overview.md",
  "exclude": [
    "**/*.test.{ts,tsx}",
    "**/*.spec.{ts,tsx}",
    "**/*.stories.{ts,tsx}",
    "**/__tests__/**",
    "**/tests/**",
    "**/__mocks__/**"
  ]
}
