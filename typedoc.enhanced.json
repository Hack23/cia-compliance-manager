{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./src/index.ts",
    "./src/typedoc-entry.ts",
    "./src/types/index.ts",
    "./src/services/index.ts",
    "./src/hooks/index.ts",
    "./src/components/index.ts",
    "./src/utils/index.ts",
    "./src/contexts/SecurityLevelContext.tsx",
    "./src/types/compliance.ts",
    "./src/types/componentPropExports.ts",
    "./src/services/securityMetricsService.ts",
    "./src/constants/index.ts",
    "./src/data/index.ts",
    "./src/application/CIAClassificationApp.tsx",
    "./src/types/cia.ts",
    "./src/types/businessImpact.ts",
    "./src/types/widgets.ts",
    "./src/services/complianceService.ts",
    "./src/services/ciaContentService.ts",
    "./src/services/businessImpactService.ts",
    "./src/services/securityResourceService.ts",
    "./src/services/technicalImplementationService.ts",
    "./src/services/BaseService.ts",
    "./src/components/widgets/index.ts",
    "./src/components/common/index.ts",
    "./src/components/charts/index.ts",
    "./src/components/securitylevel/index.ts",
    "./src/data/security/index.ts"
  ],
  "entryPointStrategy": "expand",
  "out": "./docs/api",
  "name": "CIA Compliance Manager API Documentation",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "plugin": ["typedoc-plugin-mermaid"],
  "readme": "./docs/api-overview.md",

  "theme": "default",
  "tsconfig": "./tsconfig.json",
  "sort": ["visibility", "alphabetical"],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "validation": {
    "invalidLink": false,
    "notExported": false
  },
  "searchInComments": true,
  "navigationLinks": {
    "GitHub": "https://github.com/Hack23/cia-compliance-manager",
    "Documentation": "https://hack23.github.io/cia-compliance-manager/documentation.html"
  },
  "exclude": [
    "**/*.test.{ts,tsx}",
    "**/*.spec.{ts,tsx}",
    "**/*.stories.{ts,tsx}",
    "**/*.enhanced.test.{ts,tsx}",
    "**/*.direct.test.{ts,tsx}",
    "**/*.comprehensive.test.{ts,tsx}",
    "**/*.extended.test.{ts,tsx}",
    "**/*.darkmode.test.{ts,tsx}",
    "**/__tests__/**",
    "**/tests/**",
    "**/testUtils/**",
    "**/testMocks/**",
    "**/__mocks__/**"
  ],
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core",
    "Types/CIA",
    "Types/Business",
    "Types/Widgets",
    "Types/Compliance",
    "Types/Common",
    "Types",
    "Services/Security",
    "Services/Compliance",
    "Services/Business",
    "Services",
    "Components/Widgets/Assessment",
    "Components/Widgets/Impact",
    "Components/Widgets/Implementation",
    "Components/Widgets/Business",
    "Components/Widgets",
    "Components/Common",
    "Components/Charts",
    "Components/SecurityLevel",
    "Components",
    "Hooks",
    "Utils/Security",
    "Utils/Format",
    "Utils/Risk",
    "Utils/UI",
    "Utils",
    "Data/Security",
    "Data",
    "Constants",
    "*"
  ]
}
