{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "name": "CIA Compliance Manager API Documentation",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeExternals": true,
  "plugin": ["typedoc-plugin-mermaid"],
  "readme": "./docs/api-overview.md",
  "theme": "default",
  "tsconfig": "./tsconfig.json",
  "sort": ["visibility", "alphabetical"],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "validation": {
    "invalidLink": false,
    "notExported": false
  },
  "searchInComments": true,
  "navigationLinks": {
    "GitHub": "https://github.com/Hack23/cia-compliance-manager",
    "Documentation": "https://hack23.github.io/cia-compliance-manager/documentation.html"
  },
  "exclude": [
    "**/*.test.{ts,tsx}",
    "**/*.spec.{ts,tsx}",
    "**/*.stories.{ts,tsx}",
    "**/__tests__/**",
    "**/tests/**",
    "**/__mocks__/**"
  ]
}
