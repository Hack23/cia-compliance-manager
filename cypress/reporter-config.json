{
  "reporterEnabled": "spec, mocha-junit-reporter, mochawesome",
  "mochaJunitReporterReporterOptions": {
    "mochaFile": "docs/cypress/reports/junit/results.xml",
    "toConsole": false
  },
  "mochawesomeReporterOptions": {
    "reportDir": "docs/cypress/reports/mochawesome",
    "overwrite": false,
    "html": true,
    "json": true,
    "timestamp": "mmddyyyy_HHMMss"
  }
}
